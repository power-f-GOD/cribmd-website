{
  "name": "cribmd-website",
  "scripts": {
    "dev": "next dev",
    "dev:w": "start \"\" http://localhost:3000 && next dev",
    "dev:m": "open http://localhost:3000 && next dev",
    "build": "next build",
    "start": "next start",
    "type-check": "tsc --project tsconfig.json --pretty --noEmit",
    "lint": "eslint --ext js,jsx,ts,tsx --fix",
    "postinstall": "husky install",
    "test": "jest",
    "merge:power-ui": "git pull origin staging && git checkout staging && git pull origin staging && git merge power-ui && git push && git checkout power-ui && git pull origin power-ui && git merge staging",
    "stash": "git stash save -m echo `date`",
    "stash:list": "git stash list --format='%gd (%cr): %gs'",
    "stash:update-ref": "git update-ref refs/stash stash@{0}",
    "webp": "for file in *; do cwebp $file -o ${file%.*}.webp; done",
    "jpg-rename": "for file in *.jpeg; do mv $file ${file%.*}.jpg; done",
    "img-resize": "for file in *; do magick $file -resize 250 -quality 100 ${file}; done"
  },
  "dependencies": {
    "@types/gsap": "^3.0.0",
    "@types/node": "^14.17.5",
    "@types/react": "^17.0.14",
    "@types/react-image-gallery": "^1.0.4",
    "framer-motion": "^4.1.17",
    "gsap": "^3.7.0",
    "next": "^11.0.1",
    "react": "^17.0.1",
    "react-bootstrap": "^1.6.1",
    "react-dom": "^17.0.1",
    "react-image-gallery": "^1.2.7",
    "sass": "^1.32.8",
    "typescript": "^4.3.5"
  },
  "devDependencies": {
    "@babel/core": "^7.14.6",
    "@types/jest": "^26.0.24",
    "@types/react-dom": "^17.0.9",
    "@types/react-test-renderer": "^17.0.1",
    "@typescript-eslint/eslint-plugin": "^4.28.2",
    "@typescript-eslint/parser": "^4.28.2",
    "babel-jest": "^26.6.3",
    "eslint": "^7.30.0",
    "eslint-config-next": "^11.0.1",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-jsx-a11y": "^6.4.1",
    "eslint-plugin-prettier": "^3.4.0",
    "eslint-plugin-react": "^7.24.0",
    "eslint-plugin-react-hooks": "^4.2.0",
    "husky": "^5.1.3",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^26.6.3",
    "jest-watch-typeahead": "^0.6.4",
    "lint-staged": "^10.5.4",
    "prettier": "^2.3.2",
    "react-test-renderer": "^17.0.1"
  },
  "version": "1.0.0",
  "description": "CribMD website built with Next.js",
  "main": "index.js",
  "repository": "https://github.com/CribMDPLC/cribmd-website",
  "author": "power-f-GOD <sundaypower9@gmail.com>",
  "license": "MIT",
  "private": true
}
